#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin >> n;
    int* arr = new int[n];
    for(int i = 0; i < n; i++){
        cin >> arr[i];
    }

    int max = INT_MIN;
    int smax = INT_MIN;
    
    for(int i = 0; i < n; i++){
        if(arr[i] > max){
            max = arr[i];
        }
    }

    for(int i =0; i < n; i++){
        if(arr[i] > smax && arr[i] < max){
            smax = arr[i];
        }
    }

    // it will print smax as INT_MIN if only one element is present

    cout << "Max : " << max << endl;
    cout << "Second Max : " << smax;
    return 0;

}
